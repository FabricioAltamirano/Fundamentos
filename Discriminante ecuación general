#include "stdafx.h"
#include <iostream>
#include "conio.h"
#include "math.h"
using namespace std;
void main() {
	float A, B, C, DISC;
	cout<<"ingrese el valor de A: ";
	cin>>A;
	cout<<"ingrese el valor de B: ";
	cin>>B;
	cout<<"ingrese el valor de C: ";
	cin>>C;
	DISC=pow(B,2)-4*A*C;
	cout<<"el valor del discriminante es: "<<DISC<<endl;
	getch();
}
